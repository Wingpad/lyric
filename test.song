class Int {

    private var value:N->I

    public Int(native: N->I) {
        value = native
    }

    public function plus(other: Int) {
        var ours = value
        var theirs = other.value
        return Int(N->I(*ours + *theirs))
    }
}

function main() {
    var i = Int(N->I(4))
    var j = Int(N->I(4))
    var k = i + j
}
